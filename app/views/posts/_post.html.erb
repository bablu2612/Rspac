<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <span><%= post.title %></span>
  </p>

  <p>
    <strong>Body:</strong>
    <span><%= post.body %></span>
  </p>

  <p>
    <strong>Views:</strong>
    <span><%= post.views %></span>
  </p>

  <p>
    <strong>User:</strong>
    <span><%= post.user_id %></span>
  </p>

</div>
